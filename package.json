{
  "name": "react-mobile-picker",
  "private": false,
  "version": "1.0.0",
  "description": "An mobile select box component for React",
  "type": "module",
  "keywords": [
    "react",
    "react mobile picker",
    "picker",
    "react-component",
    "antd-mobile"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/react-mobile-picker.umd.cjs",
  "module": "./dist/react-mobile-picker.js",
  "typings": "./dist/react-mobile-picker.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-mobile-picker.js",
      "require": "./dist/react-mobile-picker.umd.cjs"
    }
  },
  "repository": "https://github.com/nenio1949/react-mobile-picker",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "antd-mobile": "^5.32.2",
    "antd-mobile-icons": "^0.3.0",
    "vite-plugin-dts": "^3.5.3"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.7.0",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.3",
    "commitlint": "^17.7.1",
    "commitlint-config-gitmoji": "^2.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
